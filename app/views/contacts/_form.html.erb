<div class="container" class="input-field col s6" style="padding-top: 100px;">
  <div class="row">
    <div class="well" style="background-color: #ffffff;">
      <h2 class="text-center" style="font-family: 'Tangerine', serif;
  font-size: 48px;
  text-shadow: 4px 4px 4px #aaa;">Get in touch</h2>
      <div class="well" style="background-color: #00897b; color: #ffffff;">
        <h3 style="padding-left: 40px;">Visit Us</h3>
        <p style="padding-left: 40px;"><b>Address:</b> Gabon, Libreville
        4343 IAI Road</p>
        <p style="padding-left: 40px;"><b>Phone:</b> +241 07 78 49 86</p>
        <p style="padding-left: 40px;"><b>Email:</b> beto.nous@gmail.com</p>
      </div>
      <p>Your email address will not be published.</p>
      <%= simple_form_for(@contact) do |f| %>
      <%= f.error_notification %>
      <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

          <div class="form-inputs">
            <%= f.input :first_name, input_html: { class: 'special' }, placeholder: " Your first name", class:"form-control"%>
            <%= f.input :last_name, placeholder: " Your last name", class:"form-control", class:"required" %>
            <%= f.input :email, placeholder: " Your email", class:"form-control" %>
            <%= f.input :topic, label:"Subject", placeholder: "Subject", class:"form-control" %>
            <%# f.input :web_site, placeholder: "Web site" %>
            <%= f.input :message, placeholder: "Your message", class:"form-control" %>
          </div>

          <div class="form-actions">
            <%= f.button :submit, "Send message", class: "btn btn-primary" %>
          </div>
      <% end %>
    </div>
  </div>
</div><br>

